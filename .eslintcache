ponses\",responses)\n            setYaziDetayi(responses[0].data) // yazılardan ve yorumlardan gelen dataları ayırdık\n            setYorumlar(responses[1].data)\n        }).catch(error=> {\n            console.log(error)\n        })\n    },[]);\n    return <>\n        <h2 className=\"ui header\">{yaziDetayi.title}</h2>\n        <p>{yaziDetayi.created_at}</p>\n        <p>{yaziDetayi.content}</p>\n        <YaziYorumlari yorumlar={yorumlar} handleSubmit={handleCommentSubmit}/>\n       \n    </>\n}\nexport default YaziDetay;\n","/Users/fatih/Documents/React/yazi-yorum/src/components/YorumListesi.js",[],"/Users/fatih/Documents/React/yazi-yorum/src/components/YorumFormu.js",[],"/Users/fatih/Documents/React/yazi-yorum/src/components/YaziYorumlari.js",["48"],"import React from 'react';\nimport YorumListesi from './YorumListesi'\nimport YorumFormu from './YorumFormu';\n\nconst YaziYorumlar = (props) => {\n\n    return (\n        <>\n            <YorumListesi yorumlar={props.yorumlar}/>\n            <YorumFormu handleSubmit={props.handleSubmit}/>\n        </>\n    )\n}\n\nexport default YaziYorumlar;","/Users/fatih/Documents/React/yazi-yorum/src/components/YaziEkle.js",[],"/Users/fatih/Documents/React/yazi-yorum/src/components/YaziFormu.js",[],"/Users/fatih/Documents/React/yazi-yorum/src/api.js",[],{"ruleId":"49","replacedBy":"50"},{"ruleId":"51","replacedBy":"52"},{"ruleId":"53","severity":1,"message":"54","line":34,"column":7,"nodeType":"55","endLine":34,"endColumn":9,"suggestions":"56"},{"ruleId":"57","severity":1,"message":"58","line":1,"column":15,"nodeType":"59","messageId":"60","endLine":1,"endColumn":23},"no-native-reassign",["61"],"no-negated-in-lhs",["62"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'id'. Either include it or remove the dependency array.","ArrayExpression",["63"],"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation",{"desc":"64","fix":"65"},"Update